<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <!--<android.support.v7.widget.Toolbar xmlns:app="http://schemas.android.com/apk/res-auto"-->
    <!--android:id="@+id/toolbar"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="45dp"-->
    <!--android:gravity="center"-->
    <!--android:minHeight="45dp"-->
    <!--app:popupTheme="@style/AppTheme.PopupOverlay"-->
    <!--app:titleMarginTop="0dp">-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:background="@drawable/circular_layout_webview"
        android:padding="@dimen/default_dimen"
        android:layout_marginBottom="@dimen/default_dimen">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/closeBTN"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:src="@drawable/close"
                android:layout_marginStart="@dimen/dimen_20"
                android:layout_alignParentStart="true"
                />


            <TextView
                android:id="@+id/titleBarTxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:layout_centerInParent="true"
                android:maxLines="1" />

        </RelativeLayout>

<!--        <ImageView-->
<!--            android:id="@+id/otherOptionBTN"-->
<!--            android:layout_width="30dp"-->
<!--            android:layout_height="30dp"-->
<!--            android:layout_marginLeft="@dimen/default_dimen"-->
<!--            android:padding="@dimen/half_dimen"-->
<!--            android:src="@drawable/icon_filter"-->
<!--            android:tint="@color/medium" />-->
    </LinearLayout>
    <!--</android.support.v7.widget.Toolbar>-->

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/gray" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:indeterminateTint="@color/colorPrimary"
        android:indeterminateTintMode="src_atop" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>